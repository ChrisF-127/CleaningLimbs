<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<!-- Vacuum arm -->

	<HediffDef ParentName="AddedBodyPartBase">
		<defName>VacuumArm</defName>
		<label>vacuum arm</label>
		<labelNoun>a vacuum arm</labelNoun>
		<description>An installed vacuum arm.</description>
		<descriptionHyperlinks>
			<ThingDef>VacuumArm</ThingDef>
		</descriptionHyperlinks>
		<addedPartProps>
			<solid>true</solid>
		</addedPartProps>
		<stages>
			<li>
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.02</offset>
					</li>
				</capMods>
			</li>
		</stages>
		<spawnThingOnRemoved>VacuumArm</spawnThingOnRemoved>
	</HediffDef>

	<ThingDef ParentName="BodyPartProstheticImperialBase">
		<defName>VacuumArm</defName>
		<label>vacuum arm</label>
		<description>An arm replacement which functions like a vacuum cleaner, thereby speeding up cleaning tasks. 
While manipulation is not affected, the added weight reduces movement speed slightly.</description>
		<descriptionHyperlinks>
			<RecipeDef>InstallVacuumArm</RecipeDef>
		</descriptionHyperlinks>
		<thingSetMakerTags>
			<li>RewardStandardLowFreq</li>
		</thingSetMakerTags>
		<costList>
			<Steel>60</Steel>
			<ComponentIndustrial>8</ComponentIndustrial>
		</costList>
		<recipeMaker>
			<researchPrerequisite>SpecializedLimbs</researchPrerequisite>
		</recipeMaker>
		<techHediffsTags>
			<li>VacuumArm</li>
		</techHediffsTags>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallBodyPartProstheticBase">
		<defName>InstallVacuumArm</defName>
		<label>install vacuum arm</label>
		<description>Install a vacuum arm.</description>
		<descriptionHyperlinks>
			<ThingDef>VacuumArm</ThingDef>
			<HediffDef>VacuumArm</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing vacuum arm.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>VacuumArm</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>VacuumArm</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>Arm</li>
		</appliedOnFixedBodyParts>
		<addsHediff>VacuumArm</addsHediff>
	</RecipeDef>

